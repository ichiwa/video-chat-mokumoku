{
  "name": "video-chat-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "$(npm bin)/webpack --watch -d",
    "start:dev": "$(npm bin)/webpack --watch -d & $(npm bin)/browser-sync start --config bs-config.js",
    "build": "$(npm bin)/webpack",
    "xo": "cd ./src/; xo ; exit 0;",
    "xo:fix": "cd ./src/; xo --fix; exit 0;"
  },
  "keywords": [
    "react",
    "redux",
    "redux-saga",
    "peerjs"
  ],
  "author": "ichiwa",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.3.6",
    "axios": "^0.12.0",
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.22.0",
    "babel-register": "^6.9.0",
    "babel-root-import": "^4.0.1",
    "babel-runtime": "^6.9.2",
    "combine-section-reducers": "^1.0.3",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "moment": "^2.13.0",
    "peerjs": "^0.3.14",
    "postcss-loader": "^0.9.1",
    "react": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-css-modules": "^4.1.0",
    "react-dom": "^15.4.2",
    "react-mixin": "^3.0.5",
    "react-radio-group": "^3.0.1",
    "react-redux": "^4.4.5",
    "react-redux-i18n": "0.0.3",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.0",
    "redux-saga": "^0.10.5",
    "socket.io": "^1.4.6",
    "socket.io-client": "^1.4.6",
    "style-loader": "^0.13.1",
    "uglify-js": "^2.6.4",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "chai": "^3.5.0",
    "cpx": "^1.3.1",
    "enzyme": "^2.3.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^2.5.3",
    "mocha-webpack": "^0.3.1",
    "redux-devtools": "^3.3.1",
    "redux-logger": "^2.6.1",
    "require-dir": "^0.3.0",
    "sinon": "^1.17.4",
    "webpack-node-externals": "^1.2.0",
    "xo": "^0.17.1"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "extends": "xo-react",
    "envs": [
      "node",
      "browser"
    ],
    "plugin": [
      "react"
    ],
    "parser": "babel-eslint",
    "ignores": [
      "node_modules/**"
    ],
    "rules": {
      "no-unused-vars": 0,
      "no-negated-condition": 1,
      "camelcase": 1,
      "react/prop-types": 0,
      "react/jsx-no-bind": [
        1,
        {
          "ignoreRefs": true,
          "allowArrowFunctions": true,
          "allowBind": true
        }
      ]
    },
    "overrides": [
      {
        "files": "**components/**",
        "rules": {
          "unicorn/filename-case": 0
        }
      }
    ]
  }
}
